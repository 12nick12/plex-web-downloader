extends layout

block content
  h1= title

  table(id="example", class="table table-striped table-bordered", cellspacing="0", width="100%")
    thead
      tr
        th Nom
        th Durée
        th Année
    tbody
      each video, i in videos
        tr
          td
            a(href="/file/#{video.id}/#{video.filename}")= video.title
          td= video.second
          td= video.year
      script.
        $(document).ready(function() {
          $('#example').DataTable();
        });
